#include<iostream>
using namespace std;

int main()
{    
    int  prelim = 80;
    int midterm = 90;
    int prefinal = 95;
    int final = 85;
    float division = 4;
  
      int add = prelim+midterm+prelim+prefinal+final;
      float result = add / division;
      
      std::cout << "prelim" << "midterm" << "prefinal" << "final" << "Average" << "qou";
   
    return 0;
