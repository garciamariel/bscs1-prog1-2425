#include<iostream>
using namespace std;
int main() {

     string firstName =  "Mariel";
    string  middleName = "Tucay";
    string lastName = "Garcia";

    string fullName;

    fullName  = firstName +" "+ middleName +" "+ lastName;

    string course = "BSCS- 1";

    cout << fullName << endl;
    cout << course << endl;

    return 0;
}
